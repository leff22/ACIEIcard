{
  "version": 2,
  "name": "aciei-card",
  "alias": ["aciei-card.vercel.app"],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://acieicard-backend.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "env": {
    "VITE_API_URL": "https://acieicard-backend.onrender.com",
    "VITE_SUPABASE_URL": "https://ajalkiofvnwhtuqkrinl.supabase.co",
    "VITE_SUPABASE_ANON_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6ImFqYWxraW9mdm53aHR1cWtyaW5sIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NjMwNDY3MTQsImV4cCI6MjA3ODYyMjcxNH0.xJ0za5WqPzUXSDOp-6IQh0i5H7UAxVrWGVxdSnCBUUA"
  }
}